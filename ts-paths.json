{
  "paths": {
    "~*": ["../*"],
    "#config*": ["./*"],
    "#context*": ["../src/client/context*"],
    "#control*": ["../src/client/control*"],
    "#component*": ["../src/client/component*"],
    "#client*": ["../src/client*"],
    "#server*": ["../src/server*"],
    "#worker*": ["../src/worker*"],
    "#util*": ["../src/util*"],
    "#schema": ["../src/schema"],
    "#schema*": ["../src/schema*"],
    "#settings*": ["../src/settings*"],
    "$*": ["../src/*"],
    "%icons": ["../node_modules/@react-icons/all-files"],
    "%icons/*": ["../node_modules/@react-icons/all-files/*"]
  }
}
